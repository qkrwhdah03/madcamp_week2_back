<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"

    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/calendar"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:clipToOutline="true"
    android:background="#FFFFFF"
    tools:ignore="ExtraText">

    <!-- Auto layout, variables, and unit scale are not yet supported -->


    <CalendarView
        android:id="@+id/calendarView"
        android:layout_width="280dp"
        android:layout_height="300dp"
        android:background="@drawable/calendar_round_background"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.095"
        tools:ignore="MissingConstraints"
        />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="496dp"
        app:layout_constraintTop_toBottomOf="@+id/calendarView"

        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="-1dp">

        <RelativeLayout
            android:id="@+id/frame_10"
            android:layout_width="match_parent"
            android:layout_height="1000dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginTop="0dp"
            android:clipToOutline="true"
            >


            <RelativeLayout
                android:id="@+id/frame_11"
                android:layout_width="124dp"
                android:layout_height="36dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:layout_marginLeft="9dp"
                android:layout_marginTop="19dp"
                android:clipToOutline="true">

                <View
                    android:id="@+id/image_3"
                    android:layout_width="26dp"
                    android:layout_height="26dp"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginTop="5dp"
                    android:background="@drawable/triangle" />


                <TextView
                    android:id="@+id/foodfoodtoday"
                    android:layout_width="102dp"
                    android:layout_height="23dp"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginLeft="37dp"
                    android:layout_marginTop="7dp"
                    android:gravity="center_vertical"
                    android:text="오늘의 식단"
                    android:textAppearance="@style/some_id" />


            </RelativeLayout>

            <ImageView
                android:id="@+id/bob_morning"
                android:layout_width="88dp"
                android:layout_height="94dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:layout_marginLeft="50dp"
                android:layout_marginTop="58dp"
                android:background="@drawable/rectangle_5"
                android:src="@drawable/bob"
                android:padding="22dp"
                android:clickable="true"
                android:onClick="onImageViewClick"/>

            <ImageView
                android:id="@+id/bob_lunch"
                android:layout_width="88dp"
                android:layout_height="94dp"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="151dp"
                android:layout_alignParentTop="true"
                android:layout_marginTop="58dp"
                android:src="@drawable/bob"
                android:padding="22dp"
                android:background="@drawable/rectangle_5"
                android:clickable="true"
                android:onClick="onImageViewClick"
                />

            <ImageView
                android:id="@+id/bob_dinner"
                android:layout_width="88dp"
                android:layout_height="94dp"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="254dp"
                android:layout_alignParentTop="true"
                android:layout_marginTop="58dp"
                android:background="@drawable/rectangle_5"
                android:src="@drawable/bob"
                android:padding="22dp"
                android:clickable="true"
                android:onClick="onImageViewClick"
                />



            <RelativeLayout
                xmlns:android="http://schemas.android.com/apk/res/android"
                android:id="@+id/frame_5"
                android:layout_width="155dp"
                android:layout_height="28dp"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="5dp"
                android:layout_alignParentTop="true"
                android:layout_marginTop="227dp"
                android:clipToOutline="true"
                >
                <View
                    android:id="@+id/image_2e4"
                    android:layout_width="26dp"
                    android:layout_height="26dp"
                    android:layout_alignParentLeft="true"
                    android:layout_marginLeft="6dp"
                    android:layout_alignParentTop="true"
                    android:layout_marginTop="2dp"
                    android:background="@drawable/triangle"
                    />

                <TextView
                    android:id="@+id/some_ids"
                    android:layout_width="96dp"
                    android:layout_height="23dp"
                    android:layout_alignParentLeft="true"
                    android:layout_marginLeft="42dp"
                    android:layout_alignParentTop="true"
                    android:layout_marginTop="5dp"
                    android:text="오늘의 운동"
                    android:textAppearance="@style/some_id"
                    android:gravity="center_vertical"
                    />



            </RelativeLayout>


            <EditText
                android:id="@+id/worktext"
                android:layout_width="216dp"
                android:layout_height="180dp"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="50dp"
                android:layout_alignParentTop="true"
                android:layout_marginTop="260dp"
                android:hint="벤치프레스 2회\n덤벨 플라이 5회\n숄더프레스 4회\n레그 익스텐션 2회"
                android:textSize="15sp"
                android:background="@drawable/rectangle_2"
                android:padding="15dp"
                />


            <RelativeLayout
                xmlns:android="http://schemas.android.com/apk/res/android"
                android:id="@+id/frame_6"
                android:layout_width="155dp"
                android:layout_height="28dp"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="7dp"
                android:layout_alignParentTop="true"
                android:layout_marginTop="512dp"
                android:clipToOutline="true"
                >

                <View
                    android:id="@+id/image_2"
                    android:layout_width="26dp"
                    android:layout_height="26dp"
                    android:layout_alignParentLeft="true"
                    android:layout_marginLeft="6dp"
                    android:layout_alignParentTop="true"
                    android:layout_marginTop="2dp"
                    android:background="@drawable/triangle"
                    />

                <TextView
                    android:id="@+id/some_id"
                    android:layout_width="102dp"
                    android:layout_height="23dp"
                    android:layout_alignParentLeft="true"
                    android:layout_marginLeft="42dp"
                    android:layout_alignParentTop="true"
                    android:layout_marginTop="5dp"
                    android:text="오늘의 인바디"
                    android:gravity="center_vertical"
                    />
            </RelativeLayout>


            <EditText
                android:id="@+id/inbodytext"
                android:layout_width="254dp"
                android:layout_height="112dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:layout_marginLeft="49dp"
                android:layout_marginTop="554dp"
                android:background="@drawable/rectangle_2"
                android:hint="체중: 48kg"
                android:padding="15dp"
                android:textSize="15sp" />


        </RelativeLayout>
    </ScrollView>




</androidx.constraintlayout.widget.ConstraintLayout>
